    {
  "name": "Returns-launcher",
  "version": "1.1.0",
  "description": "Returns-launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-builder --win --ia32 -p never",
    "deploy-win": "electron-builder --win --ia32 -p always",
    "build-linux": "electron-builder --linux --x64",
    "build-mac": "electron-builder --mac --x64"
  },
  "repository": "https://github.com/bfitzgit23/Returns-Launcher",
  "homepage": "https://swgreturns.github.io",
  "keywords": [
    "hunted",
    "launcher",
    "installer",
    "swg",
    "star",
    "wars",
    "galaxies",
    "emulator",
    "emu",
    "swgemu"
  ],
  "author": "Returns_Dev",
  "license": "CC0-1.0",
  "devDependencies": {
       "electron": "^27.3.11",
        "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "fs-extra": "^11.3.0",
        "@electron/remote": "^2.1.2",
        "adm-zip": "^0.5.10",
        "electron-is-dev": "^2.0.0",
        "electron-log": "^4.4.8",
        "electron-updater": "^5.3.0",
        "follow-redirects": "^1.15.2",
        "proper-lockfile": "^4.1.2",
        "random-access-file": "^4.1.2",
        "request": "^2.88.2"
  },
 "build": {
    "appId": "com.Returns.launcher",
    "productName": "Returns Launcher",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        }
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
   }
  }
}


