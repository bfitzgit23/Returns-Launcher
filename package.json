    {
  "name": "Revenant-launcher",
  "version": "1.1.0",
  "description": Revenant-launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-builder --win --ia32 -p never",
    "deploy-win": "electron-builder --win --ia32 -p always",
    "build-linux": "electron-builder --linux --x64",
    "build-mac": "electron-builder --mac --x64"
  },
  "repository": "https://github.com/bfitzgit23/Revenant-Launcher",
  "homepage": "https://revenant.com",
  "keywords": [
    "hunted",
    "launcher",
    "installer",
    "swg",
    "star",
    "wars",
    "galaxies",
    "emulator",
    "emu",
    "swgemu"
  ],
  "author": "Revenant_Dev",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^25.2.0",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "fs-extra": "^11.3.0",
    "@electron/remote": "^2.0.9",
    "adm-zip": "^0.5.10",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.3.9",
    "follow-redirects": "^1.15.2",
    "random-access-file": "^1.8.1",
    "request": "^2.88.2"
  },
 "build": {
    "appId": "com.revenant.launcher",
    "productName": "Revenant Launcher",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        }
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
   }
  }
}
